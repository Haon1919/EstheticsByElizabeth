{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "Default": "Information",
      "Microsoft.Azure.Functions.Worker": "Warning",
      "Function.ScheduleAppointment": "Debug"    }
  },  "extensions": {
    "http": {
      "routePrefix": "api",
      "enableHttps": false
    }
  },
  "cors": {
    "allowedOrigins": ["*"],
    "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allowedHeaders": ["*"],
    "allowCredentials": false
  },
  "functionTimeout": "00:10:00"
}